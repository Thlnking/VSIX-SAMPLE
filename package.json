{
  "name": "vsix-sample",
  "displayName": "vsix-sample",
  "description": "一个个人的测试插件",
  "version": "0.5.0",
  "publisher": "yasuo",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsix-sample.ToolTip",
        "title": "提示当前项目的git账号信息",
        "shortTitle": "git账号信息"
      },
      {
        "command": "vsix-sample.showSelectionCount",
        "title": "展示当前选择行"
      }
    ],
    "keybindings": [
      {
        "command": "vsix-sample.ToolTip",
        "key": "shift+alt+i",
        "mac": "shift+cmd+i"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "ini": "^3.0.1"
  }
}
